<mapper namespace="admin">
	
<insert id="insertAdmin" parameterType="Admin">
insert into admin_info_tb values (admin_info_tb_SEQ.NEXTVAL,#{ad_id},#{ad_password},#{ad_nickname},#{ad_grade})  
</insert>
	
	
	
